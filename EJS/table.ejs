<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name = "viewport" content="width=device-widht, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Table</title>
</head>
<body>
    <h1>Tabela de Usu√°rios</h1>
    <br>
    <table border="1">
        <thead>
            <% header.forEach(col => { %>
                <th> <%= col %> </th>
            <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %>
                        <td><%= element %></td>
                    <% }) %>
                </tr>
            <% }) %> 
        </tbody>
    </table>
</body>
</html>